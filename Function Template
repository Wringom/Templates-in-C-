#include <iostream>

template<typename T>
T add(T a, T b) {
    return a + b;
}

int main() {
    int sumInt = add(5, 10);
    double sumDouble = add(2.5, 3.7);

    std::cout << "Sum of integers: " << sumInt << std::endl;
    std::cout << "Sum of doubles: " << sumDouble << std::endl;

    return 0;
}
